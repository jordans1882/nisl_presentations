\mode<presentation>

% Colors
\definecolor{msugold}{RGB}{255,186,0}
\definecolor{msublue}{RGB}{12,45,131}
\setbeamercolor{structure}{fg=msublue}

% Settings
\setbeamercolor*{title page header}{fg=white}
\setbeamercolor*{author}{fg=white}
\setbeamercolor*{date}{fg=white}
\setbeamercolor{frametitle}{fg=white, bg=msublue}


\setbeamercolor{block title}{bg=msublue,fg=white}
\setbeamercolor{block body}{use=block title,bg=block title.bg!10!bg}
\setbeamercolor{block title example}{bg=msugold,fg=white}
\setbeamercolor{block body example}{use=block title example,bg=block title example.bg!10!bg}
\setbeamercolor{block title alerted}{bg=red,fg=white}
\setbeamercolor{block body alerted}{use=block title alerted,bg=block title alerted.bg!10!bg}

\setbeamercolor{footline}{fg=msugold}
\setbeamercolor{title in head/foot}{fg=white, bg=msublue}
\setbeamercolor{date in head/foot}{fg=white, bg=msublue}

\setbeamercolor{palette primary}{fg=black, bg=msublue}
\setbeamercolor{palette secondary}{fg=white, bg=msugold}
\setbeamercolor{palette tertiary}{fg=msublue, bg=white}


\mode
<all>